{
  "name": "modethirteen/miniflex",
  "description": "A collection of PHP components to manage cryptographic keys and signatures",
  "type": "library",
  "license": "Apache-2.0",
  "authors" : [
    {
      "name": "James Andrew Vaughn",
      "email": "jamesvaughn@modethirteen.com",
      "role": "maintainer"
    }
  ],
  "support": {
    "issues": "https://github.com/modethirteen/miniflex/issues"
  },
  "require": {
    "ext-gnupg": "*",
    "ext-openssl": "*",
    "php": "~8.2.0",
    "modethirteen/type-ex": "dev-main-php82"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.9.3",
    "phpunit/phpunit": "~9.4.3",
    "rector/rector": "^0.15.1"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/MindTouch/TypeEx",
      "no-api": true
    }
  ],
  "autoload": {
    "psr-4": {
      "modethirteen\\Crypto\\": ["src/"]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "modethirteen\\Crypto\\Tests\\": ["tests/"]
    }
  }
}
